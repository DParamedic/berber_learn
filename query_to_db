USER_ID = 123123

select words.word, words.translate, words.translate_2, words.translate_3, words.notes 
from words
where words.page_id == page_id;

select dictionaries.user_id as user_id 
from dictionaries
where user_id == USER_ID;

select pages.dict_id as dict_id 
from pages
where dict_id in user_id;

